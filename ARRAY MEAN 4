import java.util.Scanner;
public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        double sum = 0;
        for (int i = 0; i < n; i++) {
            sum += sc.nextDouble();
        }
        double mean = sum / n;
        System.out.printf("Array Mean Value is %.2f%n", mean);
    }
}
